[run]
branch = True
source = .
omit =
    src/

[report]
exclude_lines =
    pragma: no cover

    @abstractmethod

    def __repr__
    if self.debug

    raise AssertionError
    raise NotImplementedError

    if 0:
    if __name__ == .__main__.:


ignore_errors = True
fail_under = 10
skip_covered = True
skip_empty = True
show_missing = True